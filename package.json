{
  "name": "@wmm387/zeroadmin",
  "type": "module",
  "version": "0.1.4",
  "packageManager": "pnpm@9.12.3",
  "author": "Zero (https://github.com/wmm387)",
  "keywords": [
    "@wmm387/zeroadmin",
    "element-plus"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/types/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./hooks": {
      "types": "./dist/types/hooks.d.ts",
      "import": "./dist/hooks.js",
      "require": "./dist/hooks.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/style.css",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "utils": [
        "./dist/types/utils.d.ts"
      ],
      "hooks": [
        "./dist/types/hooks.d.ts"
      ],
      "client": [
        "./client.d.ts"
      ]
    }
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "scripts": {
    "d": "vite --host",
    "b": "vite build && vue-tsc -p tsconfig.lib.json",
    "rel": "bumpp --no-push",
    "tsc": "tsc --noEmit && vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "up": "taze major -I"
  },
  "peerDependencies": {
    "element-plus": "^2.8.6",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/ep": "^1.2.1",
    "@iconify/vue": "^4.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.8.4",
    "@unocss/eslint-config": "^0.63.6",
    "@unocss/reset": "^0.63.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vueuse/core": "^11.1.0",
    "@wmm387/unocss-preset": "^0.0.3",
    "bumpp": "^9.8.0",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.13",
    "eslint": "^9.13.0",
    "eslint-plugin-format": "^0.1.2",
    "lodash-es": "^4.17.21",
    "sass": "^1.80.5",
    "sortablejs": "^1.15.3",
    "taze": "^0.17.2",
    "typescript": "^5.6.3",
    "unocss": "^0.63.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.10",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.1.8"
  }
}

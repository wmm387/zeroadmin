{
  "name": "@wmm387/zeroadmin",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.2.1",
  "author": "Zero (https://github.com/wmm387)",
  "keywords": [
    "@wmm387/zeroadmin",
    "element-plus"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/types/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./hooks": {
      "types": "./dist/types/hooks.d.ts",
      "import": "./dist/hooks.js",
      "require": "./dist/hooks.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/style.css",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "utils": [
        "./dist/types/utils.d.ts"
      ],
      "hooks": [
        "./dist/types/hooks.d.ts"
      ],
      "client": [
        "./client.d.ts"
      ]
    }
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "scripts": {
    "d": "vite --host",
    "b": "vite build && vue-tsc -p tsconfig.lib.json",
    "rel": "bumpp --no-push",
    "tsc": "tsc --noEmit && vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "up": "taze major -I"
  },
  "peerDependencies": {
    "element-plus": "^2.9.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@iconify-json/carbon": "^1.2.6",
    "@iconify-json/ep": "^1.2.2",
    "@iconify/vue": "^4.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.1",
    "@unocss/eslint-config": "^65.4.3",
    "@unocss/reset": "^65.4.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vueuse/core": "^12.5.0",
    "@wmm387/unocss-preset": "^0.0.4",
    "bumpp": "^10.0.2",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.13",
    "eslint": "^9.19.0",
    "eslint-plugin-format": "^1.0.1",
    "lodash-es": "^4.17.21",
    "sass": "^1.84.0",
    "sortablejs": "^1.15.6",
    "taze": "^18.4.0",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "unplugin-element-plus": "^0.9.1",
    "unplugin-vue-router": "^0.11.2",
    "vite": "^6.1.0",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.0"
  }
}
